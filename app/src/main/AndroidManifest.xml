<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dragsortlistview.example.com.dragsortlistview" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".DragSortListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider android:name="dragsortlistview.example.com.dragsortlistview.DataProvider"
                  android:authorities="dragsortlistview.example.com.dragsortlistview.DataDB"
                  android:exported="false"/>
    </application>

</manifest>
